<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ctgu.login_test">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ctgu.login_test.RangeActivity"></activity>
        <activity android:name="com.example.ctgu.login_test.DownloadActivity" />

        <service
            android:name="com.example.ctgu.login_test.DownloadService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name="com.example.ctgu.login_test.SignActivity" />
        <activity android:name="com.example.ctgu.login_test.SecondActivity" />
        <activity android:name="com.example.ctgu.login_test.Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ctgu.login_test.UsersInfo_activity" />
        <activity android:name="com.example.ctgu.login_test.ForgotInfo_activity" />
        <activity android:name="com.example.ctgu.login_test.Register_Activity" />
        <activity android:name="com.example.ctgu.login_test.pintu.PintuActivity" />
    </application>

</manifest>